class MyThread2 implements Runnable{
    public void run(){
        for (int i=0;i<=100;i++)
        System.out.println(i);
    }
}
public class ThreadDemo2{
    public static void main(String[] args){
        MyThread2 my2=new MyThread2();
            Thread t1=new Thread(my2);
        t1.start();
    }
}
